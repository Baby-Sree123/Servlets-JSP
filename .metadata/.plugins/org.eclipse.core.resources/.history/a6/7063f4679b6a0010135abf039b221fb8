package com.inh;

class A {
	int x;
}

class B extends A {
	int y;
}

class C extends A {
	int z;
}

public class EX3 {

	public static void main(String[] args) {
		B b = new B();
		C c = new C();
		
		b.x= 12;
		b.y= 27;
		
		c.x = 23;
		c.z= 23;
		
		System.out.println(b.x + " " + b.y + ", "+ c.x + " " + c.z);
		

	}

}
