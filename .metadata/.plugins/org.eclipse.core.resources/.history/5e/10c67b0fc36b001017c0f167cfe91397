package com.exp;

import java.util.Scanner;

public class EX1 {

	public static void main(String[] args) {
		Scanner c = new Scanner("System.in");
		System.out.println("Enter a number");
		int a=c.nextInt();
		System.out.println("Enter a second number");
		int b=c.nextInt();
		int res=a/b;
		System.out.println(res);
		
		
		

	}

}
